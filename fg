void nhan(int m, int n, int p)
{
	int i, j, k, s;
	for (i = 0; i < m; i++)
		for (j = 0; j < p; j++)
		{
			for (k = s = 0; k < n; k++) s += a[i][k] *b[k][j];
			c[i][j] = s;
		}
}void nhan(int m, int n, int p)
{
	int i, j, k, s;
	for (i = 0; i < m; i++)
		for (j = 0; j < p; j++)
		{
			for (k = s = 0; k < n; k++) s += a[i][k] *b[k][j];
			c[i][j] = s;
		}
}
